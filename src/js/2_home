let me = {
    age: 21,
    name: "Dima",
    surname: "Kovalkov",
    profession: "student",
    favouriteNum: 6,
    weight: 63
}

let friend = {
    age: 20,
    name: "Danya",
    surname: "Nebeskiy",
    profession: "student",
    favouriteNum: 4,
    weight: 75
}

console.log(me);
console.log(friend);

console.log(me.name == friend.name);
console.log(me.surname === friend.surname);
console.log(me.age >= friend.age);
console.log(me.profession === friend.profession);
console.log(me.favouriteNum < friend.favouriteNum);
console.log(me.weight >= friend.weight);
